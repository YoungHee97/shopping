<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shopping.web.BoardMapper">
    <select id="getBoardList" parameterType="com.shopping.web.board.form.BoardForm"
            resultType="com.shopping.web.board.dto.BoardDto">
        SELECT board_id, board_title, board_contents, board_date
        FROM board
        ORDER BY board_id DESC
    </select>
</mapper>
